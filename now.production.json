{
  "name": "analysis",
  "scope": "conveyal",
  "alias": "analysis.conveyal.com",
  "version": 2,
  "regions": ["all"],
  "builds": [{"src": "next.config.js", "use": "@now/next"}],
  "env": {
    "API_URL": "@api-url"
  },
  "build": {
    "env": {
      "ADMIN_ACCESS_GROUP": "@admin-access-group",
      "API_URL": "@api-url",
      "AUTH_DISABLED": "false",
      "AUTH0_CLIENT_ID": "@auth0-client-id",
      "AUTH0_DOMAIN": "@auth0-domain",
      "LOGROCKET": "@logrocket",
      "MAPBOX_ACCESS_TOKEN": "@mapbox-access-token"
    }
  }
}
